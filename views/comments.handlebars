<div class="conatainer">
    <div class="row">
        <div class="col-10 offset-1"></div>
        {{!--post--}}
        <div class="jumbotron jumbotron-fluid test">
            <div class="container">
                <h1 class="display-4">{{category}}</h1>
                <br><br><br><br><br><br><br><br>
                <a class="btn post" data-task="post">Learn more</a>
            </div>

        </div>
    </div>

    {{!-- posts --}}
    {{!-- add hover effect to make it look clickable --}}
    <div class="row" id="Posts">
        <div class="col-md-12">
            {{#each}}
            <div class="card get" data-post-id={{id}} data-task="post">
                <div class="card-header">
                    {{title}}
                </div>
                <div class="card story">
                    <p>{{post}}</p>
                    <div class="card-img-overlay">
                        <h5 class="card-title"></h5>
                    </div>
                </div>
                <div class="card-body">
                    <p class="card-text">{{author}}</p>
                </div>
            </div>
            {{/each}}

        </div>

    </div>

</div>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
